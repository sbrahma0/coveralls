<?xml version="1.0"?>
<package format="2">
  <name>object_collection_robotic_arm</name>
  <version>0.0.0</version>
  <description>This is the object_segregation package. It implements object
               segregation algorithm on a Kuka robot in Gazebo.</description>

  <maintainer email="ashgo95@umd.edu">Ashwin Goyal</maintainer>
  <maintainer email="anirudh@umd.edu">Anirudh Topiwala</maintainer>

  <license>BSD</license>

  <url type="website">https://github.com/Ghost1995/object_collection_robotic_arm</url>

  <author email="ashgo95@umd.edu">Ashwin Goyal</author>
  <author email="anirudh@umd.edu">Anirudh Topiwala</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>cv_bridge</build_depend>
  <build_depend>image_transport</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>rostest</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>std_msgs</build_depend>

  <build_export_depend>cv_bridge</build_export_depend>
  <build_export_depend>image_transport</build_export_depend>
  <build_export_depend>roscpp</build_export_depend>
  <build_export_depend>sensor_msgs</build_export_depend>
  <build_export_depend>std_msgs</build_export_depend>

  <exec_depend>cv_bridge</exec_depend>
  <exec_depend>image_transport</exec_depend>
  <exec_depend>roscpp</exec_depend>
  <exec_depend>rostest</exec_depend>
  <exec_depend>sensor_msgs</exec_depend>
  <exec_depend>std_msgs</exec_depend>

  <test_depend>rosunit</test_depend>
  <test_depend>gtest</test_depend>
</package>
